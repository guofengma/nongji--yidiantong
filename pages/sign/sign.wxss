
@import '../common/image_uploader.wxss';
@font-face {
    font-family: "iconfont";
    src: url(data:application/x-font-woff;charset=utf-8;base64,) format('woff');
}
.iconfont {
    font-family:"iconfont" !important;
    font-size:16px;
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-iconfonti:before { content: "\e62b"; }

.icon-shoucang:before { content: "\e666"; }
page{
    background: #f7f7f7;
}
.sign_con{
   width: 90%; padding-left:5%;
   padding-right:5%; 
   background: white;
}
.sign_con .sign_txt{ 
  border-radius:20rpx;
  padding:30rpx 0rpx;
 
 
}
.sign_con .sign_txt textarea{
  width: 100%;
  font-size: 32rpx;
  height: 80px; 
}
.btn{
  margin:auto;
  text-align:center;
}
.speciesChoose{
    line-height: 90rpx;
    background: white;
    margin: 40rpx auto;
    width: 94vw;
    display: flex;
    justify-content: space-between;
    padding: 0 10rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
}
.speciesChoose view:nth-child(1){
    display: flex;
}
.speciesChoose text{
    color: #00A700;
    font-size: 0.8em;
    margin-left: 20rpx;
}
.section{ width: 90%; margin-left: 5%; margin-right: 5%; border: 1px solid #69D4A8; border-radius: 3px; height: 3em; line-height: 3em; color: #69D4A8; margin-bottom:1em;  overflow: hidden;}
.picker{ padding-left: 1em;}
.s_title{width: 90%; margin-left: 5%; margin-right: 5%;  height:2em; line-height:2em; font-size:1.2em; color:#000; margin-bottom:1em;}
.footerbtn{ position: fixed;bottom:0;left:0;width: 100%;height: 3em; line-height: 3em; text-align: center; background: #f4f4f4; color: #00D885;}
.footerbtn button{width: 100%;
    height: 3em; line-height: 3em;
    text-align: center; background: #f4f4f4; color: #00D885; font-size:1em;}