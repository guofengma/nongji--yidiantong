page{  
  background: #f5f5f5;  
}  
/*总体主盒子*/  
.container {  
  position: relative;  
  width: 100%;  
  height: 100%;  
  background-color: #fff;  
  color: #939393;  
  overflow:hidden;

}  
.nav_top{width: 100%;display: -moz-box; 
    display: -webkit-box; 
    display: box; border-bottom: 1px solid #ccc; height:2.9em;border-top:1px solid #ccc; }
.nav_top .nav_top_l{width: 49.9%; border-right: 1px solid #ccc; box-flex: 1; text-align: center; line-height: 3em;  }
.nav_top .nav_top_l text{width: 89.9%; margin: 0 5%; display: block;margin-top: -2px;}
.nav_top .nav_top_r{width: 49.9%; box-flex: 1; text-align: center;line-height: 3em;margin-top: -2px; }
.nav_top .nav_top_r text{width: 89.9%; margin: 0 5%; display: block;}
.navtop_cur{ border-bottom: 2px solid #00D885; color: #00D885; margin-top: -.5em;}



/*左侧栏主盒子*/  
.nav_left{  
  /*设置行内块级元素（没使用定位）*/  
  display: inline-block;  
  width: 25%;  
  height: 100%;  
  /*主盒子设置背景色为灰色*/  
  background: #f5f5f5;  
  text-align: center;  
  overflow: auto;
  margin-bottom: 3.5em;
  margin-top: .3em;
}  
/*左侧栏list的item*/  
.nav_left .nav_left_items{  
  /*每个高30px*/  
  height: 30px;  
  /*垂直居中*/  
  line-height: 30px;  
  /*再设上下padding增加高度，总高42px*/  
  padding: 6px 0;  
  /*只设下边线*/  
  border-bottom: 1px solid #dedede;  
  /*文字14px*/  
  font-size: 14px;  
  overflow: hidden;
}  
/*左侧栏list的item被选中时*/  
.nav_left .nav_left_items.active{  
  /*背景色变成白色*/  
  background: #fff;  
  border-left:2px solid #00D885;
color: #00D885;
}  
  
/*右侧栏主盒子*/  
.nav_right{  
  /*右侧盒子使用了绝对定位*/  
  position: absolute;  
  top: 3.3em;  
  right: 0;  
  flex: 1;  
  /*宽度75%，高度占满，并使用百分比布局*/  
  width: 75%;  
  height: 100%;  
  padding: 10px;  
  box-sizing: border-box;  
  background: #fff;  
  overflow: auto;
  padding-bottom: 5.5em;
}  
/*右侧栏list的item*/  

.nav_right .nav_right_items{  
  /*浮动向左*/  
  float: left;  
  /*每个item设置宽度是33.33%*/  
  width: 33.33%;  
  height: 3em;  
  text-align: center; 
   border:1px solid #C0C2CE;
margin-right:1.2em;
margin-left:1.2em;
line-height:3em;
font-size:1em;
margin-bottom: 1.2em;
border-radius: 3px;
 color:#C0C2CE;
 overflow:hidden;

}  
.nav_right .nav_right_items.licur{border:1px solid #00D885;color:#fff; background:#00D885 }
.nav_right .nav_right_items image{  
  /*被图片设置宽高*/  
  width: 50px;  
  height: 30px;  
}  
.nav_right .nav_right_items text{  
  /*给text设成块级元素*/  
  display: block;   
  font-size: 14px;  
  /*设置文字溢出部分为...*/  
  overflow: hidden;  
  white-space: nowrap;  
  text-overflow: ellipsis;  
 
}  


.footerbtn{ position: fixed;bottom:0;left:0;width: 100%;height: 3em; line-height: 3em; text-align: center; background: #f4f4f4; color: #00D885;}























